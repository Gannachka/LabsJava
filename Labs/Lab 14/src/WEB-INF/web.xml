<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>jsp/index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>afternoon</servlet-name>
        <servlet-class>classespack.afternoon</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>afternoon</servlet-name>
        <url-pattern>/afternoon</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Jjj</servlet-name>
        <servlet-class>classespack.Jjj</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Jjj</servlet-name>
        <url-pattern>/Jjj</url-pattern>
    </servlet-mapping>
</web-app>